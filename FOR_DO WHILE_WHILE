public class App {
    public static void main(String[] args) throws Exception {
             //FOR LOOP
             
        for (int i = 5; i != 0; i--) {
            System.out.println("Justin Myles");
        }  
        
            int rideCount = 5;
            
            //DO WHILE
            
          do {  
            System.out.println("Ca√±edo");
            rideCount--;
        } while (rideCount != 0);
            
            int nickName = 5;
            
            //WHILE LOOP
            
            while (nickName != 0) {
            System.out.println("Tatin!");
            rideCount--;
         }
    }
}
